# Define properties
witcher:hasName a rdf:Property ;
    rdfs:domain witcher:MapPin ;
    rdfs:range xsd:string .

witcher:hasInternalName a rdf:Property ;
    rdfs:domain witcher:MapPin ;
    rdfs:range xsd:string .

witcher:locatedInWorld a rdf:Property ;
    rdfs:domain witcher:MapPin ;
    rdfs:range xsd:string .

# GeoSPARQL properties
geo:hasGeometry a rdf:Property ;
    rdfs:domain witcher:MapPin ;
    rdfs:range geo:Geometry .

geo:asWKT a rdf:Property ;
    rdfs:domain geo:Geometry ;
    rdfs:range geo:wktLiteral .
